{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@heroui/standard/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "moduleResolution": "Bundler"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
